{
  "name": "sc-next-electron",
  "version": "5.0.0",
  "description": "Electronjs com Nextjs",
  "icon": "build/icon.png",
  "description": "Aplicação ElectronJS com NextJS e typescript. #tailwindcss",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "Saulo Costa",
    "email": "saulotarsobc@gmail.com",
    "url": "https://github.com/saulotarsobc"
  },
  "license": "MIT",
  "main": "main/index.js",
  "scripts": {
    "start": "bun run build && electron .",
    "dev": "bun run build-backend && electron . --dev",
    "build-backend": "tsc -p backend",
    "build-frontend": "next build frontend",
    "postinstall": "electron-builder install-app-deps",
    "build": "bun run build-frontend && bun run build-backend",
    "rebuild": "electron-rebuild ",
    "dist:w": "bun run build && electron-builder -w",
    "dist:l": "bun run build && electron-builder -l"
  },
  "dependencies": {
    "@npmcli/fs": "^3.1.0",
    "app-root-path": "^3.1.0",
    "electron-build": "^0.0.3",
    "next": "^14.2.1",
    "react": "^18",
    "react-dom": "^18",
    "sequelize": "^6.37.3",
    "sqlite3": "4"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}